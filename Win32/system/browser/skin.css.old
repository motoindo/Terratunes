/* Menu */

/* Main Column */

.clsToolbarLabel {float:left;padding:4px;}


#dvPlayerContainer 			{height:20px;padding:1px;margin-top:6px;}
#btn_search_artist {width:24px;height:24px;background: #dddddd url(skinimg/browser/artist.gif) no-repeat 2px 2px;float:left;margin:1px;}
#btn_search_album {width:24px;height:24px;background: #e7e7e7 url(skinimg/browser/album.gif) no-repeat 2px 2px;float:left;margin:1px;}
#btn_search_song {width:24px;height:24px;background: #e7e7e7 url(skinimg/browser/song.gif) no-repeat 2px 2px;float:left;margin:1px;}
#btn_search_year {width:24px;height:24px;background: #e7e7e7 url(skinimg/browser/year.gif) no-repeat 2px 2px;float:left;margin:1px;}

#btn_keyboard_search_clear {width:24px;height:24px;background: #e7e7e7 url(skinimg/browser/btn_keyboard_search_clear.gif) no-repeat 2px 2px;float:left;margin:1px;}
#btn_keyboard_search_targeting_begin {width:24px;height:24px;background: #e7e7e7 url(skinimg/browser/btn_keyboard_search_targeting_begin.gif) no-repeat 2px 2px;float:left;margin:1px;}
#btn_keyboard_search_targeting_loose {background: #e7e7e7 url(skinimg/browser/btn_keyboard_search_targeting_loose.gif) no-repeat 2px 2px;float:left;margin:1px;}
#btn_keyboard_search_targeting_exact {width:24px;height:24px;background: #e7e7e7 url(skinimg/browser/btn_keyboard_search_targeting_exact.gif) no-repeat 2px 2px;float:left;margin:1px;}
.cls_toolbar_button_over	{border:2px solid;}
.cls_toolbar_button_down	{border:2px inset;}
.post-footer {
	background-color: #FAFAFA;
	padding: 5px; margin: 20px 10px 0 10px;
	border: 1px solid #f2f2f2;
	font-size: 95%;	
}
.post-footer .date {
	background: url(skinimg/browser/clock.gif) no-repeat left center;
	padding-left: 20px; margin: 0 10px 0 5px;
}
.post-footer .comments {
	background: url(skinimg/browser/comment.gif) no-repeat left center;
	padding-left: 20px; margin: 0 10px 0 5px;
}
.post-footer .readmore {
	background: url(skinimg/browser/page.gif) no-repeat left center;
	padding-left: 20px; margin: 0 10px 0 5px;
}

/* Sidebar */	

/* alignment classes */
.float-left  { float: left; }
.float-right { float: right; }
.align-left  { text-align: left; }
.align-right { text-align: right; }

/* display and additional classes */
.clear { clear: both; }
.gray { color: #CCC; }

/* table elements */
table {
	border-width: 0px;
	padding:10px;
	empty-cells: hide;
}

